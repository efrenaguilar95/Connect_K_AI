#include "gametree.h"

int main()
{
	//Random comment
	GameTree<int> gTree(10);
	gTree.print_value();
	GameTree<int> gTree2(8);
	//GameTree<int>* g2 = &gTree2;
	gTree.addChild(gTree2);
	gTree.first_child->print_value();
	GameTree<int> gTree3(7);
	gTree.first_child->addSibling(gTree3);
	std::cout << gTree.first_child->next_sibling->value<< std::endl;
	gTree.dfs();
}

void dfs(GameTree<T>* start_node = this)
		{
			if(start_node->first_child == NULL && start_node->next_sibling == NULL)
			{
				std::cout<<start_node->value<<std::endl;
				return;
			}

			if(start_node->first_child != NULL)
			{
				dfs(start_node->first_child);
			}

			else
			{
				std::cout<<start_node->value<<std::endl;
				if(start_node->next_sibling!= NULL)
				{
					dfs(start_node->next_sibling);
				}
				else
				{
					return;
				}
			}



		}


